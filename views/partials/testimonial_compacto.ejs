<!-- Testimoniales -->
<% for(var i=0; i<testimoniales.length; i++) {%>

  <div class="col-sm-4">
    <div class="thumbnail">
      <img src="http://placehold.it/150x150" alt="" class="img-circle">
      <div class="caption">
        <h3><%= testimoniales[i].nombre %></h3>
        <p><%= testimoniales[i].contenido %></p>
      </div>
    </div>
  </div>

<% } %>
